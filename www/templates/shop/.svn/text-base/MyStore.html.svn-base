          <div class="mystore-address" style="display:flex;justify-content:center;align-items:center;margin-top: 7px;" ng-style="addressDiv">
            <div style="display:flex;justify-content:center;align-items:center;" ng-click="addressTop()">
              <span class="address-text" ng-bind="address"></span>
              <span class="icon ion-ios-location-outline address-icon" ng-style="addressLabel"></span>
            </div>
            <div style="z-index:10;margin:-0.05rem .3rem 0 .266667rem;float: right;">
              <div class="message" style="width: 26px;height: 0.48rem;position: relative;" flex="dir:top main:center cross:center" ui-sref="ClassifyMessageCenter" ng-class="{true:'myStore-point',false:''}[flagNum]">
                  <img style="width:0.48rem;height:0.48rem;" class="message_img" ng-src="{{messageImgUrl}}">
              </div>
            </div>
          </div>
          <div>
            <img ng-src="{{coverImage|addImgURLPostfix:450:490}}" class="ms-header-img position-r" style="height: 100px;"/>
          </div>
          <div style="margin-top: 5px;width: 30px;margin-left: 5px;" class="icon ion-ios-arrow-left mystore-back-btn color-text-white"
