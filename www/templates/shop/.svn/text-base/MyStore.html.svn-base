            <!--轮播图-->
            <ion-slide-box delegate-handle="my_store_slider" does-continue="true" auto-play="true"
                           class="height-min-100" style="height: 140px;overflow: hidden;">
              <ion-slide class="height-min-100" ng-repeat="item in slideImage">
                <img class="height-min-100" height="140px" width="100%" ng-src="{{item.imageUrl|addImgURLPostfix:750:320}}" alt="轮播图"
                     ng-click="slideImageClick($index)" data-growing-idx="{{$index+1}}"/>
              </ion-slide>
            </ion-slide-box>
          </div>
          <div class="position-r margin-top-10" ui-sref="seckillList" ng-if="false">
            <img ng-src="{{imgBaseURL}}img/bg_pre.jpg" alt="秒杀" class="width-100"/>

            <div class="position-a right-10 top-10" ng-if="myStoreInfo.seckillIsHidden">
              <img ng-src="{{myStoreInfo.seckill.sgSeckillItemFormList[0].defaultImageUrl}}" alt="秒杀"
                   style="width: 50px;" class="border-white"/>
              <span class="color-text-white position-r bottom-20">更多</span>
            </div>
            <div class="bg-white border-radius position-a left-10 bottom-10 text-align-c" style="width: 66%"
                 ng-bind="countDownMsg">

            </div>
          </div>
          <ng-include src="lType"></ng-include>
          <ion-infinite-scroll ng-if="hasMoreData" immediate-check="false" on-infinite="loadMore()">
          </ion-infinite-scroll>
        </ion-content>
        <div ng-include="'templates/common/CommonLoginPopup1.html'"></div>
        <div ng-include="'templates/common/CommonLoginPopup2.html'"></div>
        <img class="myStoreButtonMain" ng-src="{{imgBaseURL}}img/ic-plus.png" ng-click="isButtonShow()"/>
        <img class="myStoreButton1" ng-src="{{imgBaseURL}}img/ic-search.png" ng-if="!buttonShow"
