          <ng-include src="tType"></ng-include>
          <div class="row row-no-padding middle-menu-bar">
            <div class="col border-right" ui-sref="homePage">
              <img ng-src="{{imgBaseURL}}img/ic_home_select.png" alt="按钮图标"/>

              <div class="font-size-12" style="color: #32beff">首页</div>
            </div>
            <div class="col  border-right" ui-sref="getCouponsList">
