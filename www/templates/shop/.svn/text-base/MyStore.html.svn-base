               <!--ng-click="goBack()" ng-style="leftArrow"></div>-->
          <!--<img ng-src="img/btn_more_white.png" alt="更多" style="width: 30px;position: absolute;top: 0;right: 5px;" ng-click="openMore()"/>-->
