  <!--二维码页面-->
  <script id="my-qrcode-modal.html" type="text/ng-template">
    <div class="ms-qrcode">
      <div class="position-r">
        <img ng-src="{{imgBaseURL}}img/ic_erweima_close.png" class="btn-close" ng-click="closeModal()"/>
      </div>
      <div class="border-bottom padding-top-bottom-10 padding-left-10">扫描二维码，分享我的顺逛店铺</div>
      <img ng-src="{{myStoreInfo.qrcode}}" alt="二维码图片"/>

