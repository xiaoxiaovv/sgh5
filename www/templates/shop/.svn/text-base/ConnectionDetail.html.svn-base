<!--二级管理-->
<ion-view view-title="人脉详细信息" class="background" hide-nav-bar="true">

  <ion-content>
    <!--合伙人头部 合伙人详细信息-->
    <div ng-style="topPadding" style="position:relative;background-image:linear-gradient(-180deg, #ec4a4a 17%, #f77c63 100%);height:244px;">
      <!--head-->
      <div style="position:relative;height:24px;padding:0 12px;">
        <div ng-click="backToLastView()" style="position:absolute;left:12px;width:24px;"><img ng-src="{{imgBaseURL}}img/connectionBack@2x.png" style="width:9px;height:16px;"></div>
        <div style="width:140px;text-align:center;font-size: 17px;color:#ffffff;position: absolute;top:0;left:0;bottom:0;right:0;margin:auto;">人脉详细信息</div>
        <!--<div><img src="img/connectionRelation@2x.png" style="width:20px;height:20px;display: none;"></div>-->
      </div>

      <!--合伙人详细信息 头像-->
      <div flex="main:left cross:center" style="margin-top:34px;">
        <div style="padding-left:28px;">
            <img ng-src="{{connectionMsg.contacts.avatarImageFileId}}" style="width:74px;height:74px;border-radius:37px;border:3px solid rgba(255,255,255,0.35);">
        </div>
        <div style="width:60%;margin-left:10px;">
          <div style="font-size:17px;color:#ffffff;letter-spacing:0px;text-align:left;">
            {{contactsStoreTopName}}的小店
          </div>
          <div style="margin-top:14px;font-size:12px;color:#ffffff;letter-spacing:0px;text-align:left;">
            注册时间：{{connectionMsg.contacts.registerTime}}
          </div>
        </div>
      </div>

      

      <!--APP 公众号-->
      <div flex="main:center cross:center" style="position:absolute;bottom:0;font-size: 16px;color:#ffffff;background:#f55b4a;width:100%;height:48px;">
        <div flex="main:center cross:center" style="width:50%;height:48px;">
          <img ng-src="{{imgBaseURL}}img/logo@2x.png" style="width:24px;height:24px;">
          <div style="margin-left:11px;font-size:14px;color:#ffffff;letter-spacing:0px;text-align:left;">
            APP 
            <span style="font-size:12px;color:#ffffff;letter-spacing:0px;text-align:center;">{{partnerMsg.contacts.downApp?'已下载':'未下载'}}</span>
          </div>
        </div>
        <div style="width:1px;height:31px;opacity:0.69;background: #ffffff;position:absolute;top:0;left:0;bottom:0;right:0;margin:auto;"></div>
        <div flex="main:center cross:center" style="width:50%;height:48px;">
          <img ng-src="{{imgBaseURL}}img/wx@2x.png" style="width:24px;height:24px;">
          <div style="margin-left:11px;font-size:14px;color:#ffffff;letter-spacing:0px;text-align:left;">
            微信公众号 
            <span style="font-size:12px;color:#ffffff;letter-spacing:0px;text-align:center;">{{partnerMsg.contacts.focusWeiXin?'已关注':'未关注'}}</span>
          </div>
        </div>
      </div>
    </div>
    <div style="font-size:16px;background: #ffffff;color:#000000;">
      <!--头像 姓名 排名-->
      <div class="connectionBorder" flex="main:center cross:center box:mean" style="position:relative;">
          <!--箭头图片-->
          <img ng-src="{{imgBaseURL}}img/next-logo@2x.png" style="position:absolute;width:15px;height:14px;left:31%;top:45px;">
          <img ng-src="{{imgBaseURL}}img/next-logo@2x.png" style="position:absolute;width:15px;height:14px;right:31%;top:45px;">
        <div flex="dir:top cross:center" style="">
            <img ng-src="{{connectionMsg.owner.avatarImageFileId}}" style="width: 60px;height: 60px;border-radius:30px;border:2px solid rgba(0,0,0,0.05);">
            <div style="margin-top:12px;font-size: 14px;padding:0 10px;">{{ownerStoreName}}</div>
            <div>
                <div style="font-size: 12px;margin-top:8px;">友圈排名：{{connectionMsg.owner.relationshipRank}}</div>
                <div style="opacity:0.54;font-size: 12px;">总排名：{{connectionMsg.owner.userRank}}</div>
            </div>
        </div>
        <div flex="dir:top cross:center" style="">
            <img ng-src="{{connectionMsg.partner.avatarImageFileId}}" style="width: 60px;height: 60px;border-radius:30px;border:2px solid rgba(0,0,0,0.05);">
            <div style="margin-top:12px;font-size: 14px;padding:0 10px;">{{partnerStoreName}}</div>
            <div>
                <div style="font-size: 12px;margin-top:8px;">友圈排名：{{connectionMsg.partner.relationshipRank}}</div>
                <div style="opacity:0.54;font-size: 12px;">总排名：{{connectionMsg.partner.userRank}}</div>
            </div>
        </div>
        <div flex="dir:top cross:center" style="">
            <img ng-src="{{connectionMsg.contacts.avatarImageFileId}}" style="width: 60px;height: 60px;border-radius:30px;border:2px solid rgba(0,0,0,0.05);">
            <div style="margin-top:12px;font-size: 14px;padding:0 10px;">{{contactsStoreName}}</div>
            <div>
                <div style="font-size: 12px;margin-top:8px;">友圈排名：{{connectionMsg.contacts.relationshipRank}}</div>
                <div style="opacity:0.54;font-size: 12px;">总排名：{{connectionMsg.contacts.userRank}}</div>
            </div>
        </div>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div>贡献佣金</div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.sumAmount}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div>店铺名称</div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.storeName}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>店</span><span>主</span><span>ID</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.memberId}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>姓</span><span>名</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.nickName}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div>开店时间</div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.createTime}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>首</span><span>单</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.isComplete}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div>友圈排名</div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.relationshipRank}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>等</span><span>级</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.order}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>总</span><span>排</span><span>名</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.userRank}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <div class="fixWidth" flex="main:justify"><span>所</span><span>在</span><span>地</span></div>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.regionName}}</span>
      </div>
      <div class="partnerDetail" flex="cross:center">
          <span>最后登录时间</span>:&nbsp;&nbsp;&nbsp;
          <span style="opacity:0.54;">{{connectionMsg.contacts.lastLoginTime}}</span>
      </div>
    </div>
    
  </ion-content>
</ion-view>
