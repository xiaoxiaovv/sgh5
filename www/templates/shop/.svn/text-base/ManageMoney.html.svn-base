  <ion-content class="background">
    <div style="background-color: #fff;">
      <div class="one-pixel-border" style="width: 100%;height: 50px;text-align: center;line-height: 50px;">
        <img ng-src="{{imgBaseURL}}img/icon-mm-recommend.png" style="width: 12px;height: 15px;">
        <span style="font-size: 16px;color:#333;">推荐好友投资返佣金（海融易）</span>
      </div>
      <div ng-repeat="item in activityInfo" class="one-pixel-border" style="width: 100%;height: 44px;line-height: 44px;font-size: 12px;color: #333;">
        <div style="padding: 0 16px;">
          <span><span>好友</span><span ng-if="item.paymentType==2">累计</span><span ng-if="item.paymentType==1">首笔</span><span>投资：</span><span>￥</span><span ng-bind="item.minPayment"></span><span>-</span><span>￥<span ng-bind="item.maxPayment"></span></span></span>
          <span style="float: right"><img ng-src="{{imgBaseURL}}img/icon-earn.png" style="width: 18px;height: 18px;position: relative;top: 2px;"><span>￥</span><span ng-bind="item.promoterRewards"></span></span>
        </div>
      </div>
      <div style="padding: 10px 16px;">
        <div style="font-size: 14px;color: #333;">活动规则</div>
        <pre style="font-size: 12px;color: #999;margin: 5px 0" ng-bind="activityRule"></pre>
      </div>
    </div>
    <div style="padding: 9px 16px;margin: 8px 0;background-color: #fff;height: 44px;line-height: 26px;">
      <div style="float:left;"><span style="color: #999;margin-right:5px;">我的推荐码</span><span ng-bind="promoCode"></span></div>
      <div ng-click="openQRModal()" style="float: right;border: 1px solid #2979FF;border-radius: 29px;height: 26px;line-height: 24px;color: #2979FF;font-size: 12px;width: 90px;text-align: center;">我的二维码</div>
    </div>
    <div style="background-color: #fff;">
      <div class="one-pixel-border" style="height: 44px;line-height: 44px;color: #333;padding: 0 16px;">
        <span style="float: left">好友七日投资记录</span>
        <span style="float:right;">我获得的佣金（元）</span>
      </div>
      <div ng-repeat="item in promotionList" class="one-pixel-border" style="height: 44px;line-height: 44px;padding: 0 16px;color: #666;">
        <div style="float: left">
          <span style="padding-right: 10px;" ng-bind="item.rewardsDate"></span><span ng-bind="item.userName"></span><span>完成投资</span>
        </div>
        <div style="float: right;" ng-bind="item.rewardsValue"></div>
      </div>
      <div ng-if="hasMoreData" ng-click="getMoreData()" style="height: 44px;line-height: 44px;background-color: #fff;padding: 0 16px;text-align: center;">
        <span style="color: #333;">查看更多</span>
        <img ng-src="{{imgBaseURL}}img/icon_lookmore.png" style="width: 16px;height: 16px;position: relative;top:3px;">
      </div>
    </div>
    <!--空间展位div-->
    <div ng-if="isApp" style="width: 100%;height: 75px;"></div>
  </ion-content>
  <!--底部分享按钮-->
  <div ng-if="isApp" style="background-color: #EEE;position:fixed;z-index: 10;bottom: 0;width: 100%;padding: 15px 0;">
    <div ng-click="openShareModal()" style="width: 90%;height: 44px;background: #2979FF;box-shadow: 0 3px 6px 0 rgba(41,121,255,0.5);border-radius: 100px;text-align: center;color:#fff;line-height: 44px;margin-left: 5%;font-size: 17px;">分享</div>
  </div>
  <!--二维码modal-->
  <script id="mm-qrcode-modal.html" type="text/ng-template">
    <div style="width: 100%;height: 267px;background-color: #fff;position: absolute;bottom: 0;z-index: 10;">
      <div style="text-align: center;font-size: 16px;color: #333;margin: 25px 0 14px;">好友在身边，请他扫一扫</div>
      <div style="width: 100%;text-align: center;">
        <div id="createdQrCode"></div>
      </div>
      <div class="one-pixel-border" style="text-align: center;">
        <span style="font-size: 12px;color:#999;">我的推荐码</span>
        <span style="font-size: 12px;color: #333;" ng-bind="promoCode"></span>
      </div>
      <div ng-click="closeQRModal()" style="background-color: #fff;height: 44px;line-height: 50px;font-size: 17px;color:#999;text-align: center;">取消</div>
    </div>
  </script>
  <!--分享modal-->
  <script id="mm-share-modal.html" type="text/ng-template">
    <div style="width: 100%;height: 210px;background-color: #fff;position: absolute;bottom: 0;z-index: 10;">
     <div style="font-size: 12px;text-align: center;margin: 21px 0 24px;">
       <span>同意</span><span ng-click="openAgreement()" style="color: #4979FF;">《自动开通天天聚服务协议》</span>
     </div>
      <div class="one-pixel-border" style="padding: 0 0 25px;height: 100px;">
        <div class="row">
          <div ng-click="shareToPlatform(1)" class="col" style="text-align: center;">
            <img ng-src="{{imgBaseURL}}img/mm-wechat.png" style="width: 45px;height: 45px;">
            <div style="font-size: 12px;color: #999;">分享给好友</div>
          </div>
          <div ng-click="shareToPlatform(2)" class="col" style="text-align: center;">
            <img ng-src="{{imgBaseURL}}img/mm-moments.png" style="width: 45px;height: 45px;">
            <div style="font-size: 12px;color: #999;">分享到朋友圈</div>
          </div>
        </div>
      </div>
      <div ng-click="closeShareModal()" style="background-color: #fff;height: 44px;line-height: 50px;font-size: 17px;color:#999;text-align: center;">取消</div>
    </div>
  </script>
