  <div class='myorderM'>
  <div class="bar sg-bar-subheader padding-left-right-0 bar-subheader" style='line-height:0.907rem;'>
    <ion-scroll scrollbar-x="false" direction="x" style="overflow:hidden;height:0.907rem;">
      <div class="row" style='padding:0;'>
        <div ng-click="changeOrder(0)" class="text-align-c col">
          	<div class=""><span ng-class="{true: 'highlight', false: ''}[orderStatus ==0]">全部</span></div>
        </div>
        <div ng-click="changeOrder(1)" class="text-align-c col">
        	<div class=""><span ng-class="{true: 'highlight', false: ''}[orderStatus ==1]">待付款</span></div>

        </div>
        <div ng-click="changeOrder(2)" class="text-align-c col">
          <div class=""><span ng-class="{true: 'highlight', false: ''}[orderStatus ==2]">待发货</span></div>

        </div>
        <div ng-click="changeOrder(3)" class="text-align-c col">
          <div class=""><span ng-class="{true: 'highlight', false: ''}[orderStatus ==3]">待收货</span></div>

        </div>
        <div ng-click="changeOrder(4)" class="text-align-c col">
          <div class=""><span ng-class="{true: 'highlight', false: ''}[orderStatus ==4]">待评价</span></div>

        </div>
      </div>
    </ion-scroll>
  </div>
  </div>
