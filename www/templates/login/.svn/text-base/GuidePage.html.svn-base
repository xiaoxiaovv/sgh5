    <div class="sg-guide-slide sg-guide-full">
      <ion-slide-box class="sg-guide-full" does-continue="true">
        <ion-slide class="text-align-c sg-guide-full">
          <img  class="sg-guide-full" ng-src="{{imgBaseURL}}img/bg_guide_1.jpg"/>
        </ion-slide>
        <ion-slide class="text-align-c">
          <img class="sg-guide-full" ng-src="{{imgBaseURL}}img/bg_guide_2.jpg"/>
        </ion-slide>
        <ion-slide class="text-align-c">
          <img class="sg-guide-full" ng-src="{{imgBaseURL}}img/bg_guide_3.jpg"/>
        </ion-slide>
        <ion-slide class="text-align-c">
          <img class="sg-guide-full" ng-src="{{imgBaseURL}}img/bg_guide_4.jpg"/>
        </ion-slide>
        <ion-slide class="text-align-c">
          <img class="sg-guide-full" ng-src="{{imgBaseURL}}img/bg_guide_5.jpg"/>
        </ion-slide>
      </ion-slide-box>
    </div>
    <div class="width-100 sg-guide-button" style="text-align: center;">
