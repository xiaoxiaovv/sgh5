<ion-view class="background" view-title="规格参数">
  <ion-content scrollbar-y="false">
    <div ng-if="flag" style="text-align:center;">
      <div style="float:left;height:195px;width:100%;">
        <img style="display:block;height:50%;margin:0 auto;margin-top:62px;" ng-src="{{imgBaseURL}}img/emptySearch.png" alt="海尔兄弟" />
      </div>
      <i style="font-size:15px;">还没有相关规格参数</i>
    </div>
      <div class="width-100 padding-0 margin-0 bg-white" ng-repeat="eachData in data">
        <div class="row border-top padding-0">
          <div class="col col-50 border-right padding-10" ng-bind="eachData.attrName" style="overflow: hidden;white-space:nowrap; text-overflow:ellipsis;"></div>
          <div class="col col-50 padding-10" ng-bind="eachData.attrValue"></div>
        </div>
      </div>
  </ion-content>
</ion-view>
