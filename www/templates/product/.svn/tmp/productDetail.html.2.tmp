  <div ng-show="isWhiteShows" style='position:fixed;top:0;bottom:0;left:0;right:0;background-color:rgba(0,0,0,0.5);z-index:100'ng-click='hideWhiteShows()'>
    <div style='position:absolute;bottom:0;left:0;right:0;background-color:rgb(255,255,255);height:450px;' ng-click='$event.stopPropagation()'>
      <div style='height:60px;border-top:1px gray solid;position:absoulte;top:0;'>
          <div style='text-align:center;padding-top:11px;line-height:20px;height:30px;'>
            <img style='width:18px;height:17px;margin-top:1px' ng-src="{{imgBaseURL}}img/Group-4.png" alt="">
