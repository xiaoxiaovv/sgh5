  <!--规格参数轮播图返回按钮-->
  <div class="icon ion-ios-arrow-left mystore-back-btn color-text-l" ng-click="showBigPicAttr()" style="position: absolute;z-index: 120;width: 24px;font-size: 38px !important;left:10px;top:20px;"
