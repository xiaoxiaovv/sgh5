        <div class=" padding-top-15" style="height: 50px;background:#fff;color: #333333;border-left: 1px solid #e4e4e4;" ng-click="openSpecNumber(2)" ng-if="(hasStock && !productModel.isActivityProduct)&&!isAcReserve">
