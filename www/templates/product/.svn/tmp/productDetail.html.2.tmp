        </div>
        <div class="padding-top-15" style="height: 50px; background: #f02864" ng-click="openSpecNumber(5)" ng-if="!hasStock&&productModel.product.bookable&&!productModel.productActivityDeposit&&(!(isAcReserve&&acReserveType==1))">
          <span>马上预订</span>
        </div>
        <div class="padding-top-15" style="height: 50px; background: #FFB300;" ng-click="openSpecNumber(6)" ng-if="!hasStock&&!productModel.product.bookable&&!productModel.productActivityDeposit&&(!(isAcReserve&&acReserveType==1))">
