#loading-ui-wrapper{
  display: none;
}

#loading-ui-image {
  position: absolute;
   top: 50%;
   left: 50%;
   width: 256px;
   height: 224px;
   margin-top: -128px; /* Half the height */
   margin-left: -126px; /* Half the width */
}

#details{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	z-index: 1;	
	display: none;
}

.imageBlock{
	width: 100%;
	height: 50%;
	position: absolute;
	z-index: 1;
	top: 0;
}

#details .rightblock{
    width: 80%;
    bottom: 1rem;
    height: 90%;
    padding: 10px;
    position: absolute;
    right: 7%;
    background: #fff;
    border-radius: 10px;
    z-index: 2;
    opacity: .7;
}
#details .rightblock #cancel{
    position: absolute;
    top: 10px;
    right: 10px;
    width: 24px;
    height:24px;
    background-image: url(http://cdn09.ehaier.com/shunguang/VRshop/image/cancel.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.id{
    width: 225px;
    padding-top:5%;
    font-size: 14px;
    width: 100%;
    font-weight: 200;
}
.featureshort{
    font-size: 1rem;
    padding: 0;
    width: 100%;
    line-height: 19px;
}
.tag,.type,.feature{
    font-size: 1rem;
    font-weight: 200;
    overflow:hidden;
  text-overflow:ellipsis;
    white-space: nowrap;
}
.tag{
    padding-top: 2%;
    padding-bottom: 2%;
}
.type{
    padding-bottom: 2%;
}

.more{
    font-size: 1rem;
    padding-top: 2%;
    width: 100%;
  text-align: right;
    color: #f39800;
}
#hongbao{
    position: absolute;
    bottom: 0.5rem;
    left: 10px;
    width: 3rem;
    height:3rem;
    background-image: url(http://cdn09.ehaier.com/shunguang/VRshop/image/control/hongbao.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.buy{
    width: 58px;
    height: 25px;
    line-height: 25px;
    background-color: #f39800;
    border-radius: 10px;
    font-size: 15px;
    text-align: center;
    margin-top: 10px;
    position: absolute;
    bottom: 0.5rem;
    right: 10px;
}


#textBlock{
    position: absolute;
    top:50%;
    width: 100%;
    height: 50%;
}


#product_pics{
	width: 50%;
	z-index: 10;
	margin-left: 25%;
}

#product_img{
	height: 90%;
	width: auto;
	display: table;
	margin:0 auto;	
}

#hongbao-open-animation {
  width: 138px; height: 138px;
  position: absolute; bottom: 70px; left: 10px;
  background: url('http://cdn09.ehaier.com/shunguang/VRshop/image/control/hongbao-spritesheet.png');
}

#back-to-origin {
    position: absolute;
    top: 10px;
    left: 10px;
    width: 64px;
    height:64px;
    background-image: url(http://cdn09.ehaier.com/shunguang/VRshop/image/control/back-to-origin-button.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
  display: block;
}


@media only screen and (max-device-height: 480px) and (orientation: landscape) {
    .featureshort{
    font-size: 15px;
    padding: 0;
    width: 100%;
    line-height: 14px;
  }
  .tag,.type,.feature{
    font-size: 12px;
    font-weight: 200;
  }
  
}

@media only screen and (max-device-height: 480px) and (orientation: portrait) {
  .featureshort{
    font-size: 1rem;
    padding: 1%;
    width: 100%;
    line-height: 1rem;
  }
  .tag,.type,.feature{
    font-size: 0.8rem;
    font-weight: 200;
    padding: 0%;
  }
  
}

@media only screen and (max-device-width: 768px) and (orientation : portrait){
	.info_text{
		font-size: 1rem;
		font-weight: 200;
		margin-bottom: 0.2em;
	}
  
}


@media only screen and (max-device-width: 768px) and (orientation : landscape){
  #details .rightblock{
    height: 80%;
}
  
	 #textBlock{
        position: absolute;
        left:50%;
        top:0;
        width: 50%;
        height: 100%;
    }

	.imageBlock{
		width: 50%;
		height: 90%;
		position: absolute;
		left: 0;
		padding-top: 2em;
	}

	.textBlock{
		width: 50%;
		height: 100%;
		position: absolute;
		right: 0;
	}

	#cash-prize{
		position: absolute;
		top: 0;
		left: 50%;
		width: 50%;
		height: 100%;
	  	display: none;
	  	z-index: 5;
	}

	#coupon-prize{
		position: absolute;
		top:0;
		left: 0;
		width: 50%;
		height: 100%;
	  	display: none;
	  	z-index: 5;
	}

	.more{
		margin-top: 1em;
	}

	.tag{
	    padding-top: 6%;
	    padding-bottom: 4%;
	}
	.type{
	    padding-bottom: 6%;
	}
  
  #hongbao-open-animation {
    width: 138px; height: 138px;
    position: absolute; bottom: 75px; left: 370px;
    background: url( 'http://cdn09.ehaier.com/shunguang/VRshop/image/control/hongbao-spritesheet.png');
  }

}