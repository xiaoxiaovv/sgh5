body{
	width: 100%;
	height: 100%;
	color:#000;
}
a{
	display: block;
	color: #fff;
	text-decoration: none;
}

#my-div{
  position: absolute;
  width: 100%;
  height: 100%;
  top:0;
  left: 0;
  display: none;
}

#overlay-blocker {
    position: fixed; 
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.1;
    filter: alpha(opacity=10);
    z-index:1;
}

#lottery-details{
  	position: absolute;
  	width: 90%;
  	height: 90%;
  	left: 5%;
	top: 5%;
	background: white;
	border-radius: 1em;
	z-index: 2;
	opacity: 0.7;
	display: none;
	line-height: 2em;
}

.lottery-msg{
	position: absolute;
	font-size: 1em;
	width: 90%;
	height: 95%;
	left: 5%;
	top:5%;	
  	overflow: scroll;
}

#lottery-cancel{
	position: absolute;
	top: 0.5em;
	right: 0.5em;
	width: 1.5em;
	height:1.5em;
	background-image: url(http://cdn09.ehaier.com/shunguang/VRshop/image/cancel.png);
	background-repeat: no-repeat;
	background-size: 100% 100%;
  	z-index: 4;
}

.lottery-msg br
{   content: "A" !important;
    display: block !important;
    margin-bottom: 1em !important;
}

