.myaccnav{
	position:relative;	
	text-align:center;
	margin-top:8px;
	margin-bottom:6px;
}
.myaccnav img{
		position:absolute;
		display: block;	    
	    left: 16px;
	}
.doimg{
	width:100%;
	height:100px;
	background:url(http://cdn09.ehaier.com/shunguang/H5/www/img/doimg.png);
	background-size:100% 100%;
	position:absolute;
	top:-8px;
	z-index:-1;
}
.mydoimg{
	width:100%;
	height:100%;
	filter:blur(10px);
	-webkit-filter:blur(10px);
	-moz-filter:blur(10px);
	-ms-filter:blur(10px);
	-o-filter:blur(10px);
	opacity: 0.5;
}
.gaimg{
	position: absolute;
    background: white;
