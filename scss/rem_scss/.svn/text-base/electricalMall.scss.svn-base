.ele_header{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:44px;
  z-index: 999;
  padding:0 4px;
  background: #f4f4f4;
  &>div:nth-child(1){
    width:40px;
  }
  &>div:nth-child(2){
    font-size: 16px;
  }
  &>div:nth-child(3){
    width:40px;
    text-align: right;
  }
  img.back{
    width:34px;
    height:34px;
    margin-top: 4px;
  }
  img.search{
    width: 28px;
    height:28px;
  }
}
.backToTop{
  width:44px;
  position: absolute;
  z-index: 999;
  bottom:60px;
  right: 20px;
}
.iosAppFixedTopEle {
  height: 64px;
  padding-top: 20px;
}

.normalFixedTopEle {
  height: 44px;
}
.iosAppTopEle{
  top:64px;
}
.normalTopEle{
  top:44px;
}
