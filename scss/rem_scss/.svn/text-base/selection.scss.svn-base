$bgColor:#ffffff;
@mixin commonTOP($color, $backColor:transparent, $pddingBottom:0px, $paddingTop:10px) {
  font-size: 17px;
  background: $backColor;
  padding-top: $paddingTop;
  padding-bottom: $pddingBottom;
  /*no*/
  color: $color;
  div:nth-child(1),
  div:nth-child(3) {
    width: 22px;
    height: 2px;
    background: $color;
    border-radius: 1px;
  }
  div:nth-child(2) {
    margin: 0 12px;
  }
}

@mixin commonBox($paddingBottom:20px) {
  width: 100%; // height: $height;
  background: $bgColor;
  margin-top: 8px;
  padding-bottom: $paddingBottom;
}

@mixin commonSlide($imgBoxWidth:27%, $imgWidth:100%, $imgHeight:101px, $titleColor:#333333, $titleFontSize:13px, $contentColor:#FF4400, $contentFontSize:13px) {
  overflow: auto;
  margin-top: 15px;
  padding-left: 10px;
  &>div {
    width: $imgBoxWidth;
    margin-right: 10px;
    img {
      width: $imgWidth;
      height: $imgHeight;
    }
    div:nth-child(2) {
      font-size: $titleFontSize;
      /*no*/
      color: $titleColor;
      @include overflowDeal();
    }
    div:nth-child(3) {
      font-size: 13px;
      /*no*/
      color: $contentColor;
      span:nth-child(2) {
        font-size: $contentFontSize;
        /*no*/
      }
    }
    div:nth-child(4) {
      background: url('http://cdn09.ehaier.com/shunguang/H5/www/img/zhuan@2x.png') center/100% no-repeat;
      width: 81px;
      height: 20px;
      padding: 4px 0 0 15px;
      text-align: center;
      color: $bgColor;
      font-size: 12px;
      /*no*/
      span {
        transform: scale(0.83, 0.83);
        margin-top: 2px;
        margin-left: -2px;
      }
    }
  }
}

@mixin middleSlide($width:100%,
$margin:11px 0 0 0) {
  width: $width;
  height: 138px;
  margin: $margin;
  img {
    width: 100%;
    height: 138px;
  }
}

@mixin overflowDeal() {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

@mixin seeMore($color) {
  margin-top: 10px;
  font-size: 13px;
  /*no*/
  color: $color;
  img {
    width: 13px;
    height: 13px;
    margin-left: 8px;
    margin-top: 2px;
  }
}

@mixin commonOnePixelBorderBottomStyle($scaleNum) {
  -webkit-transform: scaleY($scaleNum);
  transform: scaleY($scaleNum);
}

@mixin onePixelBorderBottom1() {
  @media (-webkit-min-device-pixel-ratio: 1.5), (min-device-pixel-ratio:1.5) {
    @content;
  }
}

@mixin onePixelBorderBottom2() {
  @media (-webkit-min-device-pixel-ratio: 2), (min-device-pixel-ratio:2) {
    @content;
  }
}

@mixin onePixelBorderBottom3() {
  @media (-webkit-min-device-pixel-ratio: 3), (min-device-pixel-ratio:3) {
    @content;
  }
}

