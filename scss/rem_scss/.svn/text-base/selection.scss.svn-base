.sg_notice {
  width: 100%;
  height: 50px;
  background: $bgColor;
  .sg_notice_title {
    width: 110px;
    margin-left: 14px;
    img {
      width: 110px;
      height: 22px;
    }
  }
  .sg_notice_content {
    width: 240px;
    padding-left: 10px;
    padding-right: 10px;
    font-size: 14px;
    /*no*/
    @include overflowDeal();
    position: relative;
    &>.particle {
      opacity: 0;
      position: absolute;
      animation: confetti 3s ease-in 5;
      &.c1 {
        background-color: rgba(76, 175, 80, 0.5);
      }
      &.c2 {
        background-color: rgba(156, 39, 176, 0.5);
      }
    }
  }
}
.noActiveBorderTop{
  position: relative;
  &::after{
    content: '';
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:1px;
    background: #e4e4e4;
  }
}

//礼花绽放效果
@keyframes confetti {
  0% {
    opacity: 0;
    transform: translateY(0%) rotate(0deg);
  }
  10% {
    opacity: 1;
  }
  35% {
    transform: translateY(-800%) rotate(270deg);
  }
  80% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: translateY(2000%) rotate(1440deg);
  }
}

