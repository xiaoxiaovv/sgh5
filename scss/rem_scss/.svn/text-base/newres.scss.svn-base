.yuyuepro{
	padding-bottom: 14px;
	margin-bottom:8px;
	width:100%;
	position:relative;
	background-color:white;
}
.yuyuepro>p{
	font-family:$new-font-family;
	text-indent:15px;
	span{
		font-family:$new-font-family;
	}
}
.yuyuepro>p:first-of-type{
	color:#030303;
	font-size:17px;/*no*/
	padding-top:14px;
}
.yuyuepro>p:nth-of-type(2){
	color:#999999;
	font-size:14px;/*no*/
	margin-top:8px;
	width:345px;  
	white-space: nowrap;
	text-overflow: ellipsis;
    overflow: hidden;
}
.yuyuepro>p:nth-of-type(3){
	margin-top:16px;
	span{
		display: inline-block;
		text-indent:initial;
	}
	span:first-of-type{
		font-size: 13px;/*no*/
		color:#333333;
	}
	span:nth-of-type(2),span:nth-of-type(4){
		color:#ff4400;
		font-size: 16px;/*no*/
	}
	span:nth-of-type(3),span:last-of-type{
		color:#ff4400;
	}
	img{
		width:15px;
		height:15px;
		margin-left:14px;
   		margin-right:7px;
		}
}
.yuyuepro>p:last-of-type{
	color:#999999;
	font-size:13px;/*no*/
	margin-top:2px;
}
.yuyuepro>div{
	position:absolute;
	width:100px;
	height:34px;
	background-color:#2979ff;
	color:white;
	font-size:16px;/*no*/
	line-height:34px;
	font-family:$new-font-family;
	border-radius:17px;
	right:22px;
	bottom:19px;
	text-align:center;
}
.cui-nullss{
	margin: 100px auto 0 auto;
    width: 140px;/* no */
    color: #999999;
    font-size: 14px;/* no */
}
/* 定制活动页样式 */
.customxvv_content{
		padding-bottom:5px;	
}
.showyhj{
	-webkit-flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -o-flex-wrap: wrap;
	flex-wrap: wrap;
	padding-bottom: 4px;/* no */
	&>div{
		margin-top: 10px;
		position: relative;
		width:174px;
		height:80px;
		color:white;
		div:nth-of-type(1){
			position: absolute;
			top: 15px;
			left: 23px;
			span:nth-of-type(1){
				opacity: 0.87;
				font-family: Helvetica-Light;
				margin-right: 5px;
			}
			span:nth-of-type(2){
				font-family: Impact;
				font-size: 26px;
			}
		}
		div:nth-of-type(2){
			color: rgba(255,255,255,0.87);
			position: absolute;
			bottom: 14px;
			left: 23px;
			width: 86px;
		    overflow: hidden;
		    text-overflow: ellipsis;
		    white-space: nowrap;
		}
		div:nth-of-type(3){
            position: absolute;
		    font-size: 13px;/* no */
		    right: 0;
		    top: 0;
		    width: 62px;
		    height:80px;
		    span{
		    display: block;
		    width: 30px;/* no */
		    margin: 0 auto;
		    margin-top: 22px;
		    }
		}
		.showyhjimg{
			position: absolute;
			right: 0;
			top: 0;
			width: 50px;
		}
	}
	&>div:nth-of-type(odd){
		margin-right:7px;
	}
}
.custom_list{
	height: 50px;
    line-height: 50px;
    background:white;
    font-size: 14px;/* no */
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-justify-content: space-around;
    -ms-flex-pack: distribute;
	justify-content: space-around;
	background:white;
	&>div{
		-webkit-box-flex: 1;
	    -webkit-flex: 1;
	    -ms-flex: 1;
	    flex: 1;
		span{
			display:block;
			height:50px;
			width: -webkit-fit-content;
    		width: -moz-fit-content;
			width: fit-content;
    		margin: 0 auto;
    		cursor:move;
			}
		}
}
.colis3{
	-webkit-flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -o-flex-wrap: wrap;
	flex-wrap: wrap;
	padding-bottom: 5px;
	&>div{
		width:122px;
		height: 194px;
		 position: relative;
		 background: white;
		 margin-top: 5px;
		 img{
		 	width:100%;
		 	height:117px;
		 }
		 div:nth-of-type(1){
			font-size: 13px;/* no */
			 margin-left: 9px;
			 width:104px;
			 line-height: 17px;
			 margin-top: 7px;
			 color: #333333;
			 text-overflow: ellipsis;
			 display: -webkit-box;
			 -webkit-box-orient: vertical;
			 -webkit-line-clamp: 2;
			 overflow: hidden;
		 }
		 div:nth-of-type(2){
		 	position: absolute;
		 	left:9px;
		 	color: #FF4400;
		 	bottom: 6px;
		 	span{
		 		font-size: 16px;/* no */
		 	}
		 }
		 div:nth-of-type(3){
		 	position: absolute;
		 	top: 4px;
		 	right:4px;
		 	 border: 1px solid #FF4400;/* no */
		 	 border-radius: 12px 12px 0 12px;/* no */
		 	 padding:0 6px;
		 	 height: 18px;/* no */
		 	 line-height: 16px;/* no */
		 }
	}
}
.lastmargin{
	&>div:last-of-type{
		margin-right:126px;
	}
}
