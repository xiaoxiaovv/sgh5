/* 智能家居二级页面样式---懒得新建页面了 */
.familytry{
  width:345px;
  height:135px;
  border-radius:8px;
  margin:0 auto;
  margin-top:10px;/* no */
  position:relative;
  img:nth-of-type(1){
      width:40px;
      height:40px;
      position:absolute;
      left:153px;
      top:38px;
  }
  img:nth-of-type(2){
    width:100%;
    height:30px;
    position:absolute;
    bottom:0;
    border-radius:0 0 8px 8px;
  }
  div{
    position:absolute;
    width:100%;
    bottom:6px;
    color:white;
    text-align:center;
  }
}
.familytry_one{
  background:url(http://cdn09.ehaier.com/shunguang/H5/www/img/homepage/jianyuetwo.jpg) no-repeat;
  background-size:100% 100%;
}
