.goodStart{
  background-image: linear-gradient(-180deg, #FF392F 18%, #FD592E 99%);
  .top{
    width:100%;
    height:303px;
    // padding-bottom: 69%;
    position: relative;
    .top1{
      text-align: center;
      width:76px;
      height:62px;
      transform: rotate(26deg);
      font-size: 14px;
      color: #FF122F;
      line-height: 18px;
      position: absolute;
      right:34px;
      top:73px;
    }
    .top2{
      height:28px;
      position:absolute;
      bottom:15px;
      right:21px;
      font-size: 14px;
      color: #B01600;
      line-height:28px;
      text-align:center;
      text-shadow: 0 2px 4px rgba(255,255,255,0.50);
      background-image: linear-gradient(-180deg, #FFF385 5%, #FFC807 49%, #FFAB0E 100%);
      box-shadow: 0 2px 3px 0 rgba(207,59,0,0.97);
      border-radius: 100px;
      width:26%;
    }
    .top3{
      background: #FFE41C;
      border-radius: 100px;
      font-size: 12px;
      color: #C24100;
      width:59%;
      height:28px;
      text-align:center;
      line-height:28px;
      position:absolute;
      bottom:15px;
      left:22px;
    }
    .top4{
      height:17px;
      width:2px;
      background: #FFD11C;
      border-radius: 100px;
      position:absolute;
      bottom:-2px;
    }
    .top41{
      left:37px;
    }
    .top42{
      left:225px;
    }
    .top43{
      right:36px;
    }
    .top44{
      right:104px;
    }
  }
  .content{
    position: relative;
    margin:0 16px;
    padding:20px;
    .contentDiv1{
      flex-wrap: wrap;
      padding:0 3px;
      .contentDiv11{
        position:relative;
        overflow: visible;
        box-shadow: 0 4px 0 0 #E4E0DD;
        .blackDiv{
          position:absolute;
          height:100%;
          width:100%;
          background:#000;
          box-shadow: 0 4px 0 0 #000;
          border-radius: 5px;
          opacity:0.5;
        }
        margin:4px;
        background:#fff;
        border-radius: 5px;
        img{
          width:100%;
        }
      }
    }
    .contentDiv2{
      position: absolute;
      left:50%;
      top:50%;
      transform: translate(-50%, -50%);
      width:140px;
      height:140px;
      background:red;
      border-radius: 6px;
      opacity: 0;
    }
  }
  .ruleDiv{
    margin:15px 16px 22px;
    font-size: 16px;
    color: #8B1A0A;
    border: 1px solid #B82B0D;
    border-radius: 100px;
    height:40px;
    text-align:center;
    line-height:40px;
  }
  .bottomDiv{
    height:202px;
    margin:0 5px;
    .bottomDiv1{
      height:40px;
      line-height:40px;
      text-align:center;
      font-size:15px;
      color:#FFD90E;
    }
    .bottomDiv2{
      height:152px;
      overflow: hidden;
      p{
        height:30px;
        padding:0 12px;
        line-height:30px;
        span{
          display:block;
          text-align:left;
          overflow: hidden;
          text-overflow:ellipsis;
          white-space: nowrap;
        }
        .span1{
          width:86px;
          margin-right:30px;
        }
      }
      padding:5px 32px;
      p:nth-child(odd){background:#FFE797;text-align:center;}
      p:nth-child(even){background:#FFF6C3;text-align:center;}
    }
  }
}
.helpStaffBg{
  height:100%;
  position:fixed;
  width:100%;
}
.helpStaffBg0{
  background:#000;
  opacity:0.7;
  z-index:96;
}
.helpStaffBg1{
  z-index:97;
  .helpStaffBg11{
    width:280px;
    border-radius: 8px;
    .helpStaffBg12{
      position:relative;
      img{
        width:100%;
        display:block;
      }
      span{
        font-size: 16px;
        color: #fff;
        text-shadow: 0 1px 1px rgba(0,0,0,0.12);
        position:absolute;
        top:27px;
        text-align:center;
        width:100%;
        padding:0 71px;
      }
      .goodStartBgH{
        position:absolute;
        top:91px;
        padding:0 78px;
        font-size: 14px;
        color: #fff;
        text-align:center;
      }
    }
    .helpStaffBg13{
      height:90px;
      background:#fff;
      div{
        width:212px;
        height:40px;
        background-image: linear-gradient(90deg, #FF7D3D 0%, #FE2767 100%);
        border-radius: 100px;
        font-size: 16px;
        color: #fff;
        text-align:center;
        line-height:40px;
      }
    }
  }
}
.goodStartBg{
  z-index:97;
  padding:0 6px;
  .goodStartBg1{
    margin-top:20px;
