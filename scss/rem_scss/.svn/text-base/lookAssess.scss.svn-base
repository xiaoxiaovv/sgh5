.lookAssessDiv{
  .starDiv{
    background-color: #fff;
    margin-bottom: 8px;
    padding-bottom: 10px;
    .starDivTop{
      line-height: 40px;
      padding: 0 16px;
      .starText{
        font-size: 14px;
        color: #FF4400;
        letter-spacing: 0;
      }
      .starTime{
        font-size: 12px;
        color: #999;
        letter-spacing: 0;
      }
    }
    .contentText{
      padding:0 16px;
      background-color: #fff;
      font-size: 14px;
      color: #333;
      letter-spacing: 0;
      overflow:hidden;
      text-overflow:ellipsis;
      display:-webkit-box;
      -webkit-box-orient:vertical;
      -webkit-line-clamp:2;
    }
    .assessPic{
      flex-wrap: wrap;
      background-color: #fff;
      padding-left:14px;
      .assessPicList{
        height:84px;
        margin-right:16px;
        img{
          height:56px;
          width:56px;
        }
      }
    }
  }
}
