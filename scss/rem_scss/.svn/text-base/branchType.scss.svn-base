/**
* Created by lizhi@camelotchina.com on 2017/7/15.
*
* 类别branchType页面样式
**/

.fl {
    float: left;
}

.fr {
    float: right;
}

.text-c {
    text-align: center;
}

.text-r {
    text-align: right;
}

.cl::after {
    content: '';
    display: block;
    clear: both;
}

.branchtype-has-top {
    top: 44px!important;
}

.iosContent {
    top: 64px!important;
}

.normalContent {
    top: 44px!important;
}

.branchtype-top-ios.iosAppFixedTop {
    height: 64px !important;
}

.opacityValue {
    opacity: 0;
}

.branchtype-box {
    .branchtype-heade {
        background: none!important;
        border-bottom: 1px solid #D9D9D9;
        /*no*/
        box-sizing: border-box;
        height: 44px;
        .text-gps {
            color: #999999;
            font-size: 15px;
            /*no*/
        }
        .search {
            background: #E8E8E8!important;
        }
        .text-msg {
            color: #999999;
            font-size: 12px;
            /*no*/
        }
    }
    .branchtype {
        background-color: #fff;
        .branchtype-left {
            width: 104px;
            border-right: 1px solid #D9D9D9;
            /*no*/
            height: 100%;
            padding-top: 18px;
            box-sizing: border-box;
            padding-bottom: 10px;
            margin-bottom: 10px
        }
        .branchtype-right {
            width: calc(100% - 104px);
            padding: 10px;
            height: 100%;
            margin-bottom: 10px
        }
        .branchtype-nav {
            font-size: 14px;
            /*no*/
            color: #333333;
            padding: 14px 0 14px 20px;
        }
        .branchtype-default {
            border-bottom: none;
            text-align: left;
            font-size: 16px;
            /*no*/
            color: #2979FF;
            position: relative;
            line-height: 22px;
        }
        .branchtype-default::after {
            content: "";
            position: absolute;
            left: 10px;
            top: 14px;
            height: 22px;
            width: 4px;
            background: #2979FF;
        }
        .branchtype-list dt {
            text-align: center;
            font-size: 13px;
            /*no*/
            color: #333333;
            margin-top: 24px;
        }
        .branchtype-list dt b {
            padding: 0 14px;
        }
        .branchtype-list dt span {
            height: 1px;
            /*no*/
            width: 24px;
            /*no*/
            background: #333;
            display: inline-block;
            vertical-align: middle;
        }
        .branchtype-list dd div {
            width: 33.3%;
            float: left;
            text-align: center;
            font-size: 13px;
            /*no*/
            color: #666666;
            margin-top: 20px;
        }
        .branchtype-list dd div img {
            max-width: 100%;
            width: 77px;
            height: 77px;
            margin-bottom: 12px;
        }
        .branchtype-list dd div span {
            display: block;
        }
        .border-one dd div img {
            border: 1px solid #E4E4E4;
            /*no*/
        }
        .branchtype-scroll {
            height: 100%;
            overflow: hidden;
        }
        .banner {
            height: auto;
            width: 100%;
        }
        .banner .slider-slide img {
            width: 100%;
            height: 100%;
        }
    }
}


/**
* Created by lizhi@camelotchina.com on 2017/7/15.
*
* goodsSearch页面筛选样式修改
**/

.iosAppFixedTopTwo {
    top: 64px;
}

.normalFixedTopTwo {
    top: 44px
}

.border-circular {
    font-size: 10px;
    /*no*/
    background: #FF3030;
    color: #fff;
    border-radius: 50% 50% 0 50%;
    display: inline-block;
    text-align: center;
    line-height: 18px;
    width: 18px;
    height: 18px;
    vertical-align: top;
    margin-right: 2px;
}

.color333333 {
    color: #333333;
}

.filter-modal-view {
    .search-header .title {
        color: #333;
    }
    .search-header div {
        font-size: 32px;
        /*no*/
        margin-top: 6px;
    }
    .search-content {
        margin-top: 24px;
        .search-c-box {
            margin: 0 11px;
            color: #333;
        }
        .btn-a {
            height: 30px;
            line-height: 30px;
            font-size: 13px;
            /*no*/
            text-align: center;
            width: 48%;
            position: relative;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }
        .search-c-title1 .btn-left {
            float: left;
        }
        .search-c-title1 .btn-right {
            float: right;
        }
        .search-c-title1 .btn-a {
            border: 1px solid #F5F5F5;
            /*no*/
        }
        .search-c-title1 .filter-selected {
            border: 1px solid #2979FF;
            /*no*/
        }
        .active-ioc,
        .active-ioc img {
            position: absolute;
            right: 0;
            bottom: 0;
            width: 18px;
            height: 16px;
        }
        .search-c-title2 {
            margin-bottom: 20px;
            margin-top: 20px;
        }
        .search-c-title2 .search-c-title2-h {
            margin-bottom: 20px;
            height: 14px;
            line-height: 14px;
        }
        .search-c-title2 .search-c-title2-h span {
            float: left;
            font-size: 14px;
            /*no*/
            color: #333333;
        }
        img.ioc_arrow {
            width: 11.3px;
            height: auto;
            vertical-align: middle;
            transition: 0.3s all ease;
        }
        img.ioc_arrow-down {
            transform: rotate(-90deg);
        }
        .number input {
            background: #F5F5F5;
            border-radius: 4px;
            display: inline-block;
            height: 30px;
            width: 48%;
            text-align: center;
        }
        .number span {
            display: inline-block;
            background-color: #D9D9D9;
            height: 1px;
            width: 3%;
            margin: 0 0.5%;
        }
        .search-c-title3 {
            height: 14px;
            line-height: 14px;
            margin-bottom: 20px;
            font-size: 14px;
            /*no*/
        }
        .brandBox {
            .brandList {
                border: 1px solid #F5F5F5;
                /*no*/
                background: #F5F5F5;
                border-radius: 4px;
                width: 30%;
                margin-right: 5%;
                margin-bottom: 10px;
                text-align: center;
                height: 30px;
                line-height: 28px;
                font-size: 13px;
                /*no*/
                position: relative;
                overflow: hidden;
                padding: 0 2px;
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                box-sizing: border-box;
                display: inline-block;
            }
            .brandList:nth-child(3n) {
                margin-right: 0;
            }
            .active {
                background: #fff;
                border: 1px solid #2979FF!important;
                /*no*/
                border-radius: 4px;
            }
            .brandList span {
                white-space: nowrap;
                display: block;
            }
        }
    }
}


