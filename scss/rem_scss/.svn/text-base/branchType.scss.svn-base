/*商品详情样式 2017/10/31*/
.SpecNumberModal{
  transition: all 0.2s ease-out;
  opacity: 1;
}
.modal-Spec-Closes{
  transform: translate(0,100%);
  -webkit-transform: translate(0,100%);
  .modal-backdrop-bg{ display: none;}
  opacity: 0;
}
.modal-Spec-opens{
  transform: translate(0,0);
  -webkit-transform: translate(0,0);
  .modal-backdrop-bg{ display: block;}
}

.SpecNumber-view{
  position: fixed;
  top: 153px;
  left: 0;
  background:#fff;
  overflow: inherit;
}
.SpecNumber-top{
  height: 103px;
  position: relative;
  padding-left: 9px;
  padding-right: 9px;
  .SpecNumber-top-imgs{
    position:absolute;
    left:9px;
    top:-20px;
    width: 107px;
    height: 107px;
    background: #fff;
    border: 1px solid #E4E4E4;/*no*/
    border-radius: 4px;
    padding: 5px;
    img{
      width: 100%;
      height: 100%;
    }
  }
  .SpecNumber-top-text{
    position:absolute;
    left: 126px;
    top:20px;
    .Price{font-size: 16px;/*no*/color: #FF4400; margin-bottom: 5px;}
    .texts{font-size: 13px;/*no*/color: #000000;;margin-bottom: 5px;    overflow: hidden;text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1;-webkit-box-orient: vertical;}
  }

  .top-h-1{ position: absolute; bottom: 0; left: 0;}
}
.border-h-1{width: 100%; height: 1px; background: #E4E4E4;}
.SpecNumber-content{top:103px;padding-left: 9px;padding-right: 9px;bottom: 223px;//解决规格参数太多时无法上下滚动完全的bug
  .SpecNumber-list{
     li{
       border-bottom: 1px solid #E4E4E4;
       padding-bottom: 10px;
       .title{font-size: 13px;/*no*/
              color: #000000;
              margin-top: 10px;
       }
       .add-sub-namber{
         width:96px;
         height: 36px;
         margin-top:10px;
       }
       div{
         overflow: hidden;
         span{
           background: #F5F5F5;
           border-radius: 8px;
           height: 26px;
           line-height: 26px;
           display: inline-block;
           font-size: 11px;/*no*/
           color: #333333;
           padding: 0 10px;
           float: left;
           margin-right: 10px;
           margin-top:9px;
         }
         span.active{
           background: #2979FF;
           color:#fff;
         }
         span.no-sku{
           background: #F5F5F5;
           color: #999999;
         }
       }
       .add-sub-namber-l{ margin-top: 20px;}
     }
  }
}
.SpecNumber-footer{ position: fixed; bottom:0px;left: 0;z-index: 11;width: 100%;
  .btn-list-1{height: 50px;/*no*/line-height: 50px;/*no*/ color: #fff; background: #2979FF; text-align: center;}
}


